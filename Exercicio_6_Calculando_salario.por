programa {
  funcao inicio() {
    inteiro valor, salario, horas_trab

    escreva("Digite um valor ")
    leia(valor)

    escreva(" Digite quantidade horas trabalhadas " )
    leia(horas_trab)

    salario = (horas_trab * valor)

    escreva("\nO salario é: ", salario  )
    escreva("\nA horas_trab é: ", horas_trab )


  
  }
}
